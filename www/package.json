{
  "private": true,
  "name": "www",
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "build": "toast incremental .",
    "clean": "rimraf .tmp public",
    "postinstall": "./postinstall.js"
  },
  "dependencies": {
    "@mdx-js/preact": "^2.0.0-next.8",
    "@toastdotdev/mdx": "0.0.6",
    "gray-matter": "^4.0.2",
    "react-helmet": "^6.1.0",
    "toast": "^0.3.30",
    "esinstall": "0.2.0"
  },
  "engines": {
    "node": "14.x"
  },
  "snowpack": {
    "exclude": [
      "public/**/*",
      "toast.js"
    ],
    "installOptions": {
      "alias": {
        "react": "preact/compat"
      }
    },
    "knownEntrypoints": [
      "preact",
      "preact/hooks",
      "@mdx-js/preact",
      "preact/compat",
      "react-helmet"
    ]
